{
  "manifest_version": 3,
  "name": "Disable YouTube 2x Speed",
  "version": "1.0",
  "description": "Disables the 'hold to 2x speed' feature on YouTube.",
  "host_permissions": ["*://*.youtube-nocookie.com/*", "*://*.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube-nocookie.com/*", "*://*.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    }
  }
}
